GLOBAL {
    INPUTNAME       "build/ef/2.0.prg";
    STARTADDR       $c000;    # takes start addr into account
    PAGELENGTH      0;        # No paging
    INPUTOFFS       2;
    CPU             "6502";
    COMMENTS        4;
};


# outside labels


# code segment for code and data
SEGMENT { START $c000;  END $c1ff; NAME "DATA"; };
