RANGE { START $c354;  END $c368; TYPE skip; };


LABEL { ADDR $c369; NAME "calculate_track_sector"; };
RANGE { START $c369;  END $c3b1; TYPE code; };
RANGE { START $c3b2;  END $c3d4; TYPE ByteTable; };
RANGE { START $c3d5;  END $c3f8; TYPE code; };
