# ranges
RANGE { START $c000;  END $c1ff; TYPE bytetable; };
#RANGE { START $c200;  END $c353; TYPE code; };

RANGE { START $c200;  END $c207; TYPE skip; };
RANGE { START $c208;  END $c213; TYPE skip; };

LABEL { ADDR $c215; NAME "calc_track_number"; };
